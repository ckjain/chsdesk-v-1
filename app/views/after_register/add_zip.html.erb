<h2>Thanks for signing up </h2>
<%= form_for(@user, :url => wizard_path, :method => :put) do |f| %>
  <%= f.text_field :zip, :placeholder => 'zip code' %>
  <%= f.text_field :phone_number, :placeholder => 'phone number' %>
  
  <%= f.text_field :society_name, :placeholder => 'Society Name', data: {autocomplete_source: Society.order(:name).map(&:name)} %>
  <p><%= f.submit 'Next', :class => 'btn btn-primary' %></p>
<% end %>

<%= link_to 'skip', next_wizard_path %>